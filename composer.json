{
    "name": "codicastudio/throttle",
    "description": "A noobs worthless garbage, just learning.",
    "keywords": [
        "worthless garabage by a noob."
    ],
    "homepage": "https://github.com/codicastudio/throttle",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "codicastudiodev@gmail.com",
            "homepage": "https://codicastudio.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "illuminate/cache": "^6.0 || ^7.0 || ^8.0",
        "illuminate/contracts": "^6.0 || ^7.0 || ^8.0",
        "illuminate/http": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.0",
        "graham-campbell/testbench": "^5.4",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8 || ^9.3.7"
    },
    "autoload": {
        "psr-4": {
            "codicastudio\\Throttle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "codicastudio\\Tests\\Throttle\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\Throttle\\ThrottleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
